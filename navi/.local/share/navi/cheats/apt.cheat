% apt,dpkg

# list package files from a apt package
apt-file list <package-file>

# search for a file in packages
apt-file -x search <package-file>

$ package-file: echo "" --- --header "Sorround the string by `\b` (in single quotes) for exact match"

# list files in a .deb
dpkg-deb -c <file>

$ file: ls


# list files in a installed package
dpkg -L <pkg-name>

