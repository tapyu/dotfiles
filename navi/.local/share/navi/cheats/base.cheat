% for, while, case, if

# for loop
for <element> in <elements>; do
	<commands>
done

# while loop from command
<command> | while IFS= read -r <while_var>; do
    <inner_command>
done

# while loop from file's contents (lines) as STDIN
while IFS= read -r <while_var>; do
    <command>
done < <filename>

$ while_var: echo '' --- --fzf-overrides '--query=l'
